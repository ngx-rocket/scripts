{
  "name": "@ngx-rocket/scripts",
  "version": "5.2.1",
  "description": "Support scripts for ngX-Rocket projects",
  "repository": "ngx-rocket/scripts",
  "main": "index.js",
  "bin": {
    "ngx-scripts": "./bin/ngx-scripts"
  },
  "scripts": {
    "lint": "xo \"*.js\" \"bin/*.js\"",
    "lint:fix": "npm run lint -s -- --fix",
    "test": "npm run lint -s",
    "release:check": "semantic-release --dry-run"
  },
  "keywords": [
    "ngx",
    "rocket",
    "angular",
    "cli",
    "enterprise",
    "tool",
    "support",
    "scripts",
    "project"
  ],
  "author": {
    "name": "Yohan Lasorsa",
    "url": "https://twitter.com/sinedied"
  },
  "license": "MIT",
  "homepage": "https://github.com/ngx-rocket/scripts",
  "bugs": {
    "url": "https://github.com/ngx-rocket/scripts/issues"
  },
  "dependencies": {
    "@ngx-rocket/ascii-logo": "^1.1.0",
    "chalk": "^4.1.0",
    "fs-extra": "^10.0.0",
    "lodash.get": "^4.4.2",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "semantic-release": "^18.0.1",
    "semantic-release-npm-github": "^2.0.0",
    "xo": "^0.47.0"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "xo": {
    "space": true,
    "prettier": true,
    "rules": {
      "unicorn/no-array-reduce": "off",
      "unicorn/prefer-module": "off",
      "unicorn/no-array-for-each": "off",
      "unicorn/prefer-object-from-entries": "off"
    }
  },
  "prettier": {
    "trailingComma": "none"
  },
  "release": {
    "extends": "semantic-release-npm-github",
    "branches": "main"
  },
  "files": [
    "index.js",
    "bin",
    "hooks"
  ]
}
